    <!--顶部右边圆角-->
    <attr name="topRightRadius" format="dimension" />
    <!--底部左边圆角-->
    <attr name="bottomLeftRadius" format="dimension" />
    <!--底部右边圆角-->
    <attr name="bottomRightRadius" format="dimension" />
    <!--线颜色-->
    <attr name="strokeColor" format="color" />
    <!--圆颜色-->
    <attr name="circleColor" format="color" />
    <!--线条宽度-->
    <attr name="strokeWidth" format="dimension" />
    <!--半径-->
    <attr name="radius" format="dimension" />
    <!--四角半径-->
    <attr name="cornerRadius" format="dimension" />
    <!--边缘宽-->
    <attr name="borderWidth" format="dimension" />
    <!--边缘颜色-->
    <attr name="borderColor" format="color" />
    <!--垂直间距-->
    <attr name="verticalSpacing" format="dimension" />
    <!--水平间距-->
    <attr name="horizontalSpacing" format="dimension" />
    <!--头部高度-->
    <attr name="headerHeight" format="dimension" />
    <!--脚部高度-->
    <attr name="footerHeight" format="dimension" />
    <!--刷新是否可用-->
    <attr name="refreshable" format="boolean" />
    <!--加载是否可用-->
    <attr name="loadable" format="boolean" />
    <!--持续时间-->
    <attr name="duration" format="integer" />
    <!--阴影颜色-->
    <attr name="shadowColor" format="color" />

    <!--文字是否可见-->
    <attr name="textVisibility">
        <!--可见-->
        <enum name="visible" value="0" />
        <!--隐藏-->
        <enum name="invisible" value="4" />
        <!--消失-->
        <enum name="gone" value="8" />
    </attr>

    <!--图形-->
    <attr name="shape">
        <!--矩形-->
        <enum name="rectangle" value="0"></enum>
        <!--椭圆-->
        <enum name="oval" value="1"></enum>
        <!--线条-->
        <enum name="line" value="2"></enum>
        <!--圆圈-->
        <enum name="ring" value="2"></enum>
    </attr>

    <!--加载控件-->
    <declare-styleable name="LoadingView">
        <!--半径-->
        <attr name="android:radius" />
        <!--方向-->
        <attr name="android:orientation" />
        <!--文字大小-->
        <attr name="android:textSize" />
        <!--文字-->
        <attr name="android:text" />
        <!--文字颜色-->
        <attr name="android:textColor" />
        <!--文字可见性-->
        <attr name="textVisibility" />
        <!--线条颜色-->
        <attr name="streakColor" format="color" />
        <!--开始透明度-->
        <attr name="startAlpha" format="integer" />
        <!--结束透明度-->
        <attr name="endAlpha" format="integer" />
        <!--动画持续时间-->
        <attr name="duration" />
        <!--单位角度-->
        <attr name="unitAngle" format="float" />
        <!--线条宽度-->
        <attr name="streakWidth" format="dimension" />
        <!--线条间距-->
        <attr name="streakSpace" format="dimension" />
    </declare-styleable>

    <!--状态-->
    <declare-styleable name="StatusView">
        <!--类型-->
        <attr name="status" format="integer">
            <!--网络-->
            <enum name="wireless" value="0"></enum>
            <!--成功-->
            <enum name="success" value="1"></enum>
            <!--失败-->
            <enum name="warning" value="-1"></enum>
        </attr>
        <!--半径-->
        <attr name="radius" />
        <!--是否使用动画-->
        <attr name="isAnimator" format="boolean" />
        <!--颜色-->
        <attr name="android:color" format="color" />
        <!--圆线条宽度-->
        <attr name="circleStrokeWidth" format="dimension" />
        <!--警告上方半径-->
        <attr name="warningTopRadius" format="dimension" />
        <!--警告下方半径-->
        <attr name="warningBottomRadius" format="dimension" />
        <!--警告圆点半径-->
        <attr name="warningDotRadius" format="dimension" />
        <!--网络线条宽度-->
        <attr name="wirelessStrokeWidth" format="dimension" />
        <!--网络层数-->
        <attr name="wirelessCount" format="integer" />
        <!--网络开始半径-->
        <attr name="wirelessStartRadius" format="dimension" />
        <!--网络内间距-->
        <attr name="wirelessPadding" format="dimension" />
        <!--网络开始角度-->
        <attr name="wirelessStartAngle" format="float" />
    </declare-styleable>

    <!--刷新基础布局-->
    <declare-styleable name="SwipeLayout">
        <!--头部高度-->
        <attr name="headerHeight" />
        <!--脚部高度-->
        <attr name="footerHeight" />
        <!--是否可刷新-->
        <attr name="refreshable" />
        <!--是否可加载-->
        <attr name="loadable" />
        <!--缩放持续时间，单位毫秒-->
        <attr name="scaleDuration" format="integer" />
        <!--动画延迟开始时间，单位毫秒-->
        <attr name="delayDuration" format="integer" />
        <!--移动持续时间，单位毫秒-->
        <attr name="translateDuration" format="integer" />
    </declare-styleable>

    <!--刷新请求布局-->
